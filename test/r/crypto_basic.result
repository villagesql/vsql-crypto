SET @plaintext = 'Hello, World!';
SET @key = 'my-secret-key-16';
SET @encrypted = encrypt(@plaintext, @key, 'aes');
SELECT decrypt(@encrypted, @key, 'aes') = @plaintext;
decrypt(@encrypted, @key, 'aes') = @plaintext
1
SET @data = 'The quick brown fox jumps over the lazy dog';
SET @key256 = 'this-is-a-32-byte-key-for-aes';
SET @enc = encrypt(@data, @key256, 'aes-256');
SELECT decrypt(@enc, @key256, 'aes-256') = @data;
decrypt(@enc, @key256, 'aes-256') = @data
1
